User
  name Text
  email Text Maybe
  password Text Maybe
  verkey Text Maybe
  verified Bool default=False
  admin Bool default=False
  superAdmin Bool default=False
  realUser Bool default=True
  pdgaNumber Int Maybe
  UniqueUser email !force
  deriving Typeable
  deriving Show

Competition
  userId UserId
  layoutId LayoutId
  date Day
  name Text
  playerLimit Int
  state CompetitionState
  password Text
  serieId SerieId Maybe
  pdga Bool default=False
  private Bool default=False
  locked Bool default=False
  UniqueCompetition layoutId date name
  deriving Show

Serie
  name Text
  UniqueSerie name
  deriving Show

SignUp
  userId UserId
  competitionId CompetitionId
  confirmed Bool default=False
  division Division
  UniqueSignUp userId competitionId
  deriving Show

Round
  userId UserId
  competitionId CompetitionId
  state RoundState
  roundnumber Int
  groupnumber Int
  penalty Int
  UniqueRound userId competitionId roundnumber
  deriving Show

Score
  roundId RoundId
  holeId HoleId
  score Int
  time UTCTime
  UniqueScore roundId holeId
  deriving Show

ScoreUpdateLog
  scoreId ScoreId
  competitionId CompetitionId
  time UTCTime
  old Int
  new Int
  deriving Show

Course
  name Text
  UniqueCourse name
  deriving Show

Layout
  courseId CourseId
  name Text
  description Text
  UniqueLayout courseId name
  deriving Show

Hole
  layoutId LayoutId
  number Int
  par Int
  UniqueHole layoutId number
  deriving Show

Notification
  content Textarea
  user UserId
  date UTCTime
  deriving Show

Permission
  userId UserId
  type PermissionType
  deriving Show

EventLog
  userId UserId
  time UTCTime
  level Level
  event Text
  deriving Show

CompetitionDivision
  competitionId CompetitionId
  division Division
  deriving Show

CompetitionEventLog
  competitionId CompetitionId
  time UTCTime
  level Level
  event Text
  deriving Show