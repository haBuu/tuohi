<div .container>

  <div .panel .panel-default>
    <div .panel-heading>_{MsgRound}: #{currentRound}

  <form method=post action=@{CompetitionNextRoundR cid}#form enctype=#{nextRoundFormEnctype}>
    ^{nextRoundFormWidget}

  $forall group <- groups
    <div .panel .panel-default>
      <div .panel-heading>_{MsgGroup} #{group}
      <table .table>
        <tbody>
          $forall (E.Value rid, E.Value _, E.Value groupNumber, E.Value name) <- rounds
            $if group == groupNumber
              <tr>
                <td>
                  <p>#{name}
                <td>
                  <div .pull-right>
                    <button data-url=@{DnfRoundR rid} #dnf-round type="button" class="btn btn-danger btn-sm">
                      <span class="glyphicon glyphicon-remove">


  <ul .list-group #dnf-list>
    <div .panel .panel-default>
      <div .panel-heading>_{MsgDNF}
      $forall (E.Value rid, E.Value name) <- dnf
        <li .list-group-item>#{name}

  <form method=post action=@{CompetitionFinishR cid}#form enctype=#{finishCompetitionFormEnctype}>
    ^{finishCompetitionFormWidget}

  <a href=@{AdminR} .btn .btn-default .btn-block .btn-lg>_{MsgBack}