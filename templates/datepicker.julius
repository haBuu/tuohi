(function() {

  $('input[type="date"]').datepicker({
    format: "yyyy-mm-dd",
    weekStart: 1,
    startDate: "today",
    endDate: "+1y",
    todayBtn: "linked",
    language: #{toJSON lang},
    calendarWeeks: true,
    todayHighlight: true,
    disableTouchKeyboard: true
  });

})();